// simple scalars are on stack.

a := 10;
b := 20;

c := true;

while a = 0 and b = 0 do
    if a / 2 = 0 then
        a := a + 2;
        b := b - 3;
    else
        a := a - 1;
        b := b + 2;
    end
end

def fib (n) do if n <= 1 then 1 else fib(n - 1) + fib(n - 2) end;

def main() do
    a := fib(10);
end;
